{
  "name": "photography-portfolio",
  "version": "1.0.0",
  "description": "Portfolio website for a director of photography",
  "main": "index.js",
  "scripts": {
    "start": "eleventy --serve",
    "build": "eleventy",
    "build:css": "tailwindcss -i ./src/css/tailwind.css -o ./_site/css/styles.css --minify",
    "watch:css": "tailwindcss -i ./src/css/tailwind.css -o ./_site/css/styles.css --watch",
    "dev": "concurrently \"npm run start\" \"npm run watch:css\""
  },
  "keywords": [
    "photography",
    "portfolio",
    "eleventy",
    "tailwind"
  ],
  "author": "Sophia Dengo",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2"
  }
}
